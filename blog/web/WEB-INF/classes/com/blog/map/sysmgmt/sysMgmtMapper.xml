<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.sysmgmt">
	<!-- 获取后台管理菜单SQL -->
	<select id="getSysMenu" resultType="com.blog.modal.SysMenu">
		SELECT
		SM.ID,
		SM.CODE,
		SM.NAME,
		SM.PARENT_ID,
		SM.CREATETIME,
		SM.SEQ,
		SM.URL
		FROM
		sys_menu SM
		ORDER BY SM.SEQ;
	</select>
</mapper>  
